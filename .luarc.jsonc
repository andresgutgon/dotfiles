{
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": ["vim", "Snacks", "oil"]
  },
  "workspace": {
    "library": [
      "${3rd}/luv/library",
      "${3rd}/busted/library",
      "/usr/share/nvim/runtime/lua",
      "/usr/share/nvim/runtime/lua/vim/lsp",
      "/usr/share/nvim/runtime/lua/vim",
      "~/.local/share/nvim/lazy/snacks.nvim",
      "~/.local/share/nvim/lazy/oil.nvim"
    ],
    "checkThirdParty": false
  },
  "telemetry": {
    "enable": false
  }
}
